{
  "name": "@riktar/frontman",
  "description": "Opinionated CLI for automate frontend task",
  "version": "0.0.1",
  "author": "Riccardo Tartaglia @riktar",
  "bin": {
    "frontman": "./bin/run"
  },
  "bugs": "https://github.com/riktar/frontman/issues",
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "globby": "^10.0.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/riktar/frontman",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "frontman",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found"
    ]
  },
  "repository": "riktar/frontman",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "@oclif/plugin-not-found": "^1.2.4",
    "chalk": "^2.4.2",
    "handlebars": "^4.7.7",
    "inquirer": "^6.5.2",
    "npm-commands": "^1.1.0",
    "ora": "^5.4.1"
  }
}
