import React from "react";
{{#each routes}}
import {{this.key}} from "pages/{{this.key}}";
{{/each}}
import jsonRoutes from "./router.json";

const Component = {
  {{#each routes}}
    {{this.key}},
  {{/each}}
}

const routes = jsonRoutes.map((x) => ({
  ...x,
  component: React.createElement(Component[x.key], {})
}));


export { routes };
